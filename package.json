{
  "name": "emere-graphql",
  "version": "1.0.0",
  "description": "GraphQL Server for E-MERE",
  "private": true,
  "scripts": {
    "start": "npm-run-all pm2:*",
    "build": "npm run clean && tsc && npm run lint",
    "lint": "tslint -c tslint.json 'src/**/*.ts",
    "gql-gen": "npm-run-all create:typings-folder watch:graphql-schema",
    "tsc": "tsc",
    "clean": "rimraf ./dist",
    "pm2:start-app": "pm2 start ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "create:typings-folder": "node ./scripts/create-typings-folder.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "watch:graphql-schema": "gql-gen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://zeferinix@bitbucket.org/brader/emere-graphql.git"
  },
  "keywords": [
    "emere",
    "e-mere",
    "node",
    "graphql",
    "javascript",
    "typescript"
  ],
  "author": "Cerino O. Ligutom III <zeferinix@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/brader/emere-graphql#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/express-winston": "^3.0.0",
    "@types/graphql": "^14.0.4",
    "@types/graphql-depth-limit": "^1.1.0",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "eslint": "^5.11.1",
    "graphql-code-generator": "^0.15.2",
    "graphql-codegen-introspection": "^0.15.2",
    "graphql-codegen-typescript-common": "0.15.2",
    "graphql-codegen-typescript-resolvers": "0.15.2",
    "graphql-codegen-typescript-server": "0.15.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "bcrypt": "^3.0.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-winston": "^3.0.1",
    "graphql": "^14.0.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^3.0.0",
    "graphql-shield": "^5.1.0",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "moment": "^2.23.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.11",
    "winston": "^3.1.0"
  }
}
