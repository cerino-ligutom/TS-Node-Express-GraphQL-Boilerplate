{
  "name": "emere-graphql",
  "version": "1.0.0",
  "description": "GraphQL Server for E-MERE",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js && npm run logs",
    "build": "npm run clean && tsc && npm run lint",
    "lint": "tslint -c tslint.json 'src/**/*.ts",
    "tsc": "tsc",
    "clean": "rimraf ./dist",
    "logs": "pm2 logs EMERE-GraphQL",
    "migration:create": "node ./scripts/typeorm/migration-create",
    "migration:generate": "node ./scripts/typeorm/migration-generate",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://zeferinix@bitbucket.org/brader/emere-graphql.git"
  },
  "keywords": [
    "emere",
    "e-mere",
    "node",
    "graphql",
    "javascript",
    "typescript"
  ],
  "author": "Cerino O. Ligutom III <zeferinix@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/brader/emere-graphql#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "eslint": "^5.11.1",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2"
  },
  "dependencies": {
    "bcrypt": "^3.0.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.0",
    "pg": "^7.7.1",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.11"
  }
}
