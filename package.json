{
  "name": "tsnode-express-apollo-graphql-starter",
  "version": "1.0.0",
  "description": "Apollo GraphQL Server Starter project",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "production": "ts-node ./src/app.ts",
    "build": "npm run clean && tsc && npm run lint",
    "lint": "tslint -p tsconfig.json",
    "gql-gen": "npm-run-all create:typings-folder watch:graphql-schema",
    "tsc": "tsc",
    "clean": "rimraf ./dist",
    "create:typings-folder": "node ./scripts/create-typings-folder.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "watch:graphql-schema": "gql-gen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerino-ligutom/TS-Node-Express-GraphQL-Starter.git"
  },
  "bugs": {
    "url": "https://github.com/cerino-ligutom/TS-Node-Express-GraphQL-Starter/issues"
  },
  "keywords": [
    "node",
    "typescript",
    "tslint",
    "ts-node",
    "express",
    "graphql",
    "apollo-graphql",
    "graphql-shield",
    "facebook-dataloader",
    "passport",
    "typeorm",
    "postgres",
    "winston",
    "bcrypt",
    "gql-gen"
  ],
  "author": "Cerino O. Ligutom III <zeferinix@gmail.com>",
  "license": "GPL-3.0",
  "homepage": "https://github.com/cerino-ligutom/TS-Node-Express-GraphQL-Starter",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/express-winston": "^3.0.0",
    "@types/graphql": "^14.0.4",
    "@types/graphql-depth-limit": "^1.1.0",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/shortid": "0.0.29",
    "eslint": "^5.11.1",
    "graphql-code-generator": "~0.16.0",
    "graphql-codegen-introspection": "~0.16.0",
    "graphql-codegen-typescript-common": "~0.16.0",
    "graphql-codegen-typescript-resolvers": "~0.16.0",
    "graphql-codegen-typescript-server": "~0.16.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "bcrypt": "^3.0.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-winston": "^3.0.1",
    "graphql": "^14.0.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^3.0.0",
    "graphql-shield": "^5.1.0",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "moment": "^2.23.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1",
    "reflect-metadata": "^0.1.12",
    "shortid": "^2.2.14",
    "typeorm": "^0.2.11",
    "winston": "^3.1.0"
  }
}
